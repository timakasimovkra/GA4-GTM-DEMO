      let domestic_announcement_bar = document.querySelector('.INF-Announcements');
      let domestic_only = document.querySelector('[domestic_only]');
      let domestic_only_mobile = document.querySelector('[domestic_only_mobile]');
      let international_only = document.querySelector('[international-checkbox]');
      let international_only_mobile = document.querySelector('[international-checkbox_mobile]');
      
      if (Flow.getExperience()) {
        int_announcement_bar.style.visibility = 'visible';
        domestic_announcement_bar.style.visibility = 'hidden';
        domestic_announcement_bar.style.height = 0;
        
        if (document.querySelectorAll('[hide-international]')) {
          document.querySelectorAll('[hide-international]').forEach((component) => {
            component.style.display = 'none';
          });
        }

        if (document.querySelectorAll('[international-checkbox]')) {
          document.querySelectorAll('[international-checkbox]').forEach((checkbox) => {
            checkbox.checked = false;
          });
        }


        if (international_only_mobile) {
          if (country === 'AUS') {
            international_vat_text.innerText = '(Duties and GST included)';
          } else if (country === 'MEX') {
            international_vat_text.innerText = '(Duties and VAT included)';
          } else if (country === 'CAN') {
            international_vat_text.innerText = '(Duties and Taxes applied at checkout)';
          } else if (country === 'NZL') {
            international_vat_text.innerText = '(Duties and GST applied at checkout)';
          } else if (country === 'GBR') {
            international_vat_text.innerText = '(VAT included)';
          }
        }


        Flow.cart.localize({
          success: (data) => {
            const shippingValues = data.flow.order.promotions.available[0] ? data.flow.order.promotions.available[0].trigger.min : data.flow.order.promotions.applied[0].trigger.min;

            const shippingThresholdValue = shippingValues.amount;
            const shippingThresholdCurrency = shippingValues.currency;
            let shippingThresholdLabel = shippingValues.label;
            const freeShippingMessage = document.querySelector('[int-freeshipping-msg]');

            window.theme.free_shipping_bar.free_shipping_threshold = shippingThresholdValue;
            if (document.querySelector('[data-free-shipping-threshold]')) {
                document.querySelector('[data-free-shipping-threshold]').dataset.freeShippingThreshold = shippingThresholdValue;
            }


            if (freeShippingMessage) {
              let shipping_type = "Standard";
              
              if (country === 'MEX' || country === 'SGP' || country === 'HKG' || country === 'JPN' || country === 'AUS' || country == "NOR") {
                shipping_type = 'Express';
              }
              freeShippingMessage.innerHTML = `Free ${shipping_type} Shipping Over ${shippingThresholdLabel}`;
            }
          },
          error: (error) => console.error(error)
        });
      } else {         
        domestic_announcement_bar.style.visibility = 'visible';
        int_announcement_bar.style.visibility = 'hidden';
        int_announcement_bar.style.height = 0;

        if (document.querySelectorAll('[hide-domestic]')) {
          document.querySelectorAll('[hide-domestic]').forEach((component) => {
            component.style.display = 'none';
          });
        }

        if (document.querySelectorAll('[hide-international]')) {
          document.querySelectorAll('[hide-international]').forEach((component) => {
            component.style.display = 'block';
          });
        }

        if (document.querySelectorAll('[international-checkbox]')) {
          document.querySelectorAll('[international-checkbox]').forEach((checkbox) => {
            checkbox.checked = true;
          });
        }
      }  
      const newsletterSelector = document.querySelectorAll('.NewsletterSignup__country-picker select');
      newsletterSelector.forEach((selector) => {
        const newsletterOptions = selector.options;
          for (var opt, i = 0; opt = newsletterOptions[i]; i++) {
            if (opt.dataset.country === country) {
              selector.selectedIndex = i;
              break;
            }
        }
      })
    }
    
    // Edit Back to Cart on Flow Checkout
    function replaceReturnToCart() {
      const topCartLink = document.querySelector('[data-automation-id=cart-breadcrumb] a');
      const lowerCartLink = document.querySelector('.customer-information-form__desktop-cart-button');
      if (topCartLink) {
        let topParent = topCartLink.parentElement;
        topCartLink.remove();
        // Desktop Optimizely Test
        if (window.innerWidth > 767) {
           topParent.innerHTML = '<a class="step__title" href="/cart"><span>Cart</span></a>';
        } else {
          topParent.innerHTML = '<a class="step__title" href="/#cart"><span>Cart</span></a>';
        }

      }
      if (lowerCartLink) {
        let lowerParent = lowerCartLink.parentElement;
        lowerCartLink.remove();
           if (window.innerWidth > 767) {
              lowerParent.innerHTML = '<a class="button button button--flat-default button--medium customer-information-form__flat-cart-button" href="/cart"><span>Return to Cart</span></a>';
          } else {
            lowerParent.innerHTML = '<a class="button button button--flat-default button--medium customer-information-form__flat-cart-button" href="/#cart"><span>Return to Cart</span></a>';
        }
 
      }
    }

    if (window.flow && window.flow.checkout) {
      const checkoutEnums = flow.checkout.enums.pageView;
      flow.checkout.onPageView(checkoutEnums.ALL, replaceReturnToCart);
    }

    Flow.set('on', 'pageview.checkout_step_1', replaceReturnToCart);
    Flow.set('on', 'pageview.checkout_step_2', replaceReturnToCart);
    Flow.set('on', 'pageview.checkout_step_3', replaceReturnToCart);
    
  });
