<script>

  function start() {
    var select = document.getElementById('lang');
    select.addEventListener("change", addActivityItem);
  }

  function addActivityItem() {
    var en = document.querySelectorAll(".en");
    var fr = document.querySelectorAll(".fr");
    if (this.value == "en") {
      for (let i = 0; i < en.length; i++) {
        en[i].classList.remove("dlt");
      }
      for (let i = 0; i < fr.length; i++) {
        fr[i].classList.add("dlt");
      }
    } else {
      for (let i = 0; i < en.length; i++) {
        en[i].classList.add("dlt");
      }
      for (let i = 0; i < fr.length; i++) {
        fr[i].classList.remove("dlt");
      }
    }
  }

  window.addEventListener("load", start, false);


</script>
  
